        xmlns:local="clr-namespace:CGA4"
        xmlns:wpfx="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:oxy="http://oxyplot.org/wpf" x:Class="CGA4.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="688.367" Width="990" ResizeMode="NoResize">
